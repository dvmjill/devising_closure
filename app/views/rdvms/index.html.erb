<p id="notice"><%= notice %></p>

<h1>Rdvms</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Clinic name</th>
      <th>Clinic address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Pref contact</th>
      <th>Phone</th>
      <th>Email</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rdvms.each do |rdvm| %>
      <tr>
        <td><%= rdvm.first_name %></td>
        <td><%= rdvm.last_name %></td>
        <td><%= rdvm.clinic_name %></td>
        <td><%= rdvm.clinic_address %></td>
        <td><%= rdvm.city %></td>
        <td><%= rdvm.state %></td>
        <td><%= rdvm.zip %></td>
        <td><%= rdvm.pref_contact %></td>
        <td><%= rdvm.phone %></td>
        <td><%= rdvm.email %></td>
        <td><%= rdvm.user_id %></td>
        <td><%= link_to 'Show', rdvm %></td>
        <td><%= link_to 'Edit', edit_rdvm_path(rdvm) %></td>
        <td><%= link_to 'Destroy', rdvm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rdvm', new_rdvm_path %>
